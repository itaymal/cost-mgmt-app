# GCP Configuration
REACT_APP_GCP_PROJECT_ID=your-gcp-project-id

# Option 1: API Key (Simpler setup)
REACT_APP_GCP_API_KEY=your-gcp-api-key

# Option 2: Service Account JSON Key (More secure)
# Copy the entire JSON content from your downloaded service account key file
REACT_APP_GCP_SERVICE_ACCOUNT_KEY={"type":"service_account","project_id":"your-project-id","private_key_id":"...","private_key":"-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n","client_email":"your-service-account@your-project.iam.gserviceaccount.com","client_id":"...","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"https://www.googleapis.com/robot/v1/metadata/x509/your-service-account%40your-project.iam.gserviceaccount.com","universe_domain":"googleapis.com"}

REACT_APP_GCP_API_BASE_URL=https://cloudbilling.googleapis.com/v1

# Azure Configuration (for future use)
REACT_APP_AZURE_SUBSCRIPTION_ID=your-azure-subscription-id
REACT_APP_AZURE_TENANT_ID=your-azure-tenant-id
REACT_APP_AZURE_CLIENT_ID=your-azure-client-id

# API Configuration
REACT_APP_API_BASE_URL=https://your-api-endpoint.com
REACT_APP_ENABLE_MOCK_DATA=true

# Dashboard Configuration
REACT_APP_DEFAULT_TIME_RANGE=6m
REACT_APP_REFRESH_INTERVAL=300000
REACT_APP_MAX_RESOURCES_PER_PAGE=100
